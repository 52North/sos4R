	OWS-methods-parsing.R SOS-methods-parsing.R OM-methods.R
	OM-methods-coercion.R OM-methods-parsing.R SA-methods.R
	GML-methods.R SWE-methods.R SML-methods.R GML-methods-parsing.R
	SA-methods-parsing.R SWE-methods-parsing.R OGC-methods.R 
	SOS-methods-accessor.R PrintShowStructureSummmary-methods.R
